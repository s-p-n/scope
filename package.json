  "license": "MIT",
  "bugs": {
    "url": "https://github.com/s-p-n/arini/issues"
  },
  "homepage": "https://github.com/s-p-n/arini#readme",
  "dependencies": {
    "browserify-fs": "^1.0.0",
    "chalk": "^1.1.3",
    "combine-source-map": "^0.8.0",
    "convert-source-map": "^1.5.1",
    "express": "^4.16.3",
    "js-beautify": "^1.7.5",
    "meow": "^5.0.0",
    "mongojs": "^2.6.0",
    "shelljs": "^0.8.2",
    "socket.io": "^2.1.1",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.6",
    "spdy": "^3.4.7",
    "xregexp": "^4.2.0"
  },
  "devDependencies": {
    "dateformat": "^3.0.3",
    "jison": "^0.4.18",
    "man-api": "^0.3.1",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3"
  }
